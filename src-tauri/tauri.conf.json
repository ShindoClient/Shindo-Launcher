{
  "productName": "Shindo Launcher",
  "version": "1.5.0",
  "identifier": "com.shindo.launcher",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "bundle": {
    "active": true,
    "category": "Game",
    "copyright": "ShindoLauncher",
    "publisher": "Miki IT Services",
    "icon": [
      "icons/icon.ico",
      "icons/icon_256x256.png",
      "icons/icon_128x128.png",
      "icons/icon_64x64.png",
      "icons/icon_32x32.png",
      "icons/icon_24x24.png",
      "icons/icon_16x16.png"
    ],
    "shortDescription": "A ShindoClient launcher for Minecraft",
    "longDescription": "A ShindoClient launcher for the game Minecraft",
    "license": "GPL-3.0",
    "licenseFile": "../LICENSE",
    "resources": [],
    "targets": "all",
    "createUpdaterArtifacts": "v1Compatible",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "",
      "webviewInstallMode": {
        "type": "embedBootstrapper"
      }
    },
    "linux": {
      "appimage": {
        "bundleMediaFramework": true
      }
    },
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    }
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "ShindoLauncher",
        "label": "main",
        "width": 1000,
        "minWidth": 1000,
        "height": 670,
        "minHeight": 670,
        "resizable": true,
        "fullscreen": false,
        "transparent": true,
        "decorations": false,
        "center": true,
        "windowEffects": {
          "effects": [
            "acrylic"
          ]
        },
        "shadow": true
      }
    ]
  },
  "plugins": {
    "updater": {
      "active": true,
      "endpoints": [
        "https://github.com/ShindoClient/Shindo-Launcher/releases/latest/download/latest.json"
      ],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEIwQTI4QjkxNDI0M0E1QzEKUldUQnBVTkNrWXVpc0xzNERKb2oyeXFYU1pyWFdaY1N5UDZEelJEVHM1Q0VYWFZxUkh3Y1hocjkK"
    }
  }
}
